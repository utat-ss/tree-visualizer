{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "client: ng serve",
      "type": "pwa-chrome",
      "request": "launch",
      "preLaunchTask": "npm: start",
      "url": "http://localhost:4200/"
    },
    {
      "name": "client: ng test",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "npm: test",
      "url": "http://localhost:9876/debug.html"
    },
    {
      "type": "node-terminal",
      "name": "client: npm run start",
      "request": "launch",
      "command": "npm run start",
      "cwd": "${workspaceFolder}/server"
    }
  ]
}